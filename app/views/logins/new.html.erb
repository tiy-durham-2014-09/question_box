<h1>Log In or <%= link_to 'Register', new_user_path %></h1>
<p id="notice"><%= notice %></p>


<%= form_tag login_path do %>

  <%= label_tag :email %>
  <%= text_field_tag :email %>
  <br>
  <%= label_tag :password %>
  <%= password_field_tag :password %>
  <br>
  <%= submit_tag "Log In" %>
<% end %>